            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".gui.estagiario.home.perfil.ActPerfilEstagiario" />
        <activity android:name=".gui.empregador.home.perfil.PerfilEmpregador" />
