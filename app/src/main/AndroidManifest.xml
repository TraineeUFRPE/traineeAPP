            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name=".gui.estagiario.gui.Login.LoginEstagiario"
            android:screenOrientation="portrait" />
        <activity
            android:name=".gui.empregador.login.LoginEmpregador"
